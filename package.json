{
    "name": "@wont/react-ui",
    "version": "0.0.0",
    "description": "UI Component build on React",
    "main": "lib/es/index.js",
    "module": "lib/es/index.js",
    "typings": "lib/cjs/index.d.ts",
    "files": [
        "lib"
    ],
    "sideEffects": false,
    "scripts": {
        "build": "ts-node scripts/build.ts",
        "gulp": "gulp -f scripts/gulpfile.ts",
        "rb": "rm -rf lib && rollup -c scripts/rollup.config.ts",
        "prettier": "prettier --check .",
        "prettier:writer": "prettier --write .",
        "eslint": "eslint --ext .tsx .",
        "eslint:fix": "eslint --ext .tsx,.js . --fix",
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "start-storybook -p 6006",
        "build:docs": "build-storybook -o docs",
        "commit": "git-cz"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wont-org/react-ui.git"
    },
    "keywords": [
        "@wont",
        "react",
        "components",
        "ui-components",
        "@wont/react-ui"
    ],
    "author": {
        "name": "liukun",
        "email": "919590347@qq.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/wont-org/react-ui/issues"
    },
    "homepage": "https://wont-org.github.io/react-ui",
    "devDependencies": {
        "@commitlint/cli": "^9.1.1",
        "@commitlint/config-conventional": "^9.1.1",
        "@rollup/plugin-commonjs": "^15.0.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^9.0.0",
        "@storybook/addon-actions": "^6.0.5",
        "@storybook/addon-essentials": "^6.0.5",
        "@storybook/addon-links": "^6.0.5",
        "@storybook/react": "^6.0.5",
        "@types/gulp": "^4.0.6",
        "@typescript-eslint/eslint-plugin": "^3.9.0",
        "@typescript-eslint/parser": "^3.9.0",
        "commitizen": "^4.1.2",
        "css-loader": "^4.2.1",
        "cz-conventional-changelog": "^3.2.0",
        "eslint": "^7.6.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-react": "^7.20.6",
        "eslint-plugin-react-hooks": "^4.0.8",
        "glob": "^7.1.6",
        "gulp": "^4.0.2",
        "gulp-autoprefixer": "^7.0.1",
        "gulp-cssnano": "^2.1.3",
        "gulp-less": "^4.0.1",
        "husky": "^4.2.5",
        "less-loader": "^6.2.0",
        "lint-staged": "^10.2.11",
        "prettier": "^2.0.5",
        "rimraf": "^3.0.2",
        "rollup": "^2.26.5",
        "rollup-plugin-less": "^1.1.2",
        "rollup-plugin-terser": "^7.0.0",
        "rollup-plugin-typescript2": "^0.27.2",
        "through2": "^4.0.2",
        "ts-node": "^9.0.0",
        "typescript": "^3.9.7"
    },
    "dependencies": {
        "prop-types": "^15.7.2",
        "react": "^16.13.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "*": "prettier --writer",
        "*.(jsx?|tsx?)": [
            "eslint --cache --fix"
        ]
    }
